<?xml version="1.0" encoding="utf-8"?>
<manifest package="com.example.android.fractal"
          xmlns:android="http://schemas.android.com/apk/res/android">

    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>

    <application
        android:allowBackup="true"
        android:icon="@drawable/iconlogo"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:theme="@style/Theme.DesignDemo">
        </activity>
        <activity
            android:name=".SplashScreen"
            android:theme="@style/SplashTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".LoginActivity">
        </activity>
        <activity
            android:name=".RegistrationActivity"
            android:theme="@style/AppTheme">
        </activity>
        <activity android:name=".EmployeeImageUpload">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".FetchEmployeeStatusActivity" />
        </activity>
        <activity android:name=".EmployeeVisaApplicationActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".FetchEmployeeStatusActivity" />
        </activity>
        <activity android:name=".FetchEmployeeStatusActivity">
        </activity>
        <activity android:name=".AdminApplicationListActivity">
        </activity>
        <activity android:name=".NextActivity">
        </activity>
        <activity android:name=".AdminApplicationDecisionActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".NextActivity" />
        </activity>
        <activity android:name=".LawyerNextActivity">
        </activity>
        <activity android:name=".LawyerApplicationListActivity">
        </activity>
        <activity android:name=".LawyerApplicationDecisionActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".LawyerNextActivity" />
        </activity>
        <activity android:name=".LawyerImageDeatils">
        </activity>
        <activity android:name=".LawyerImagesDisplay">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".LawyerApplicationListActivity" />
        </activity>
        <activity android:name=".FourthActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".LawyerApplicationDecisionActivity" />
        </activity>
        <activity android:name=".ManagerApplicationListActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".FetchEmployeeStatusActivity" />
        </activity>
        <activity android:name=".ManagerNextActivity">
        </activity>
        <activity android:name=".ManagerAuthenticationActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".FetchEmployeeStatusActivity" />
        </activity>
        <activity android:name=".ManagerApplicationDecisionActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".ManagerNextActivity" />
        </activity>
        <activity android:name=".ManagerCommentsActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".FetchEmployeeStatusActivity" />
        </activity>
        <activity android:name=".AdminCommentsActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".FetchEmployeeStatusActivity" />
        </activity>
        <activity android:name=".LawyerCommentsActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".FetchEmployeeStatusActivity" />
        </activity>
    </application>

</manifest>
